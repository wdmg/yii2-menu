{
  "name": "wdmg/yii2-menu",
  "description": "Menu module",
  "keywords": ["yii2", "yii2-menu", "user menu", "wdmg"],
  "type": "yii2-extension",
  "license": "MIT",
  "version": "1.2.1",
  "homepage": "https://github.com/wdmg/yii2-menu",
  "support": {
    "source": "https://github.com/wdmg/yii2-menu",
    "issues": "https://github.com/wdmg/yii2-menu/issues"
  },
  "authors": [
    {
      "name": "Alexsander Vyshnyvetskyy",
      "email": "alex.vyshnyvetskyy@gmail.com"
    },
    {
      "name": "W.D.M.Group, Ukraine",
      "email": "wdmg.com.ua@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0.40",
    "wdmg/yii2-base": "^1.3.1",
    "wdmg/yii2-helpers": "^1.4.5",
    "wdmg/yii2-validators": "^1.0.7",
    "wdmg/yii2-selectinput": "^1.0.9"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "wdmg\\menu\\": ""
    }
  },
  "extra": {
    "options": {
      "routePrefix": "admin",
      "supportModels": {
        "pages": "wdmg\\pages\\models\\Pages",
        "news": "wdmg\\news\\models\\News",
        "blog": "wdmg\\blog\\models\\Posts"
      },
      "supportLocales": ["ru-RU", "uk-UA", "en-US"]
    }
  }
}